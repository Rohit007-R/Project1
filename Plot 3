plot(t$Sub_metering_1 ~ t$DateTime, type = 'l', ylab = 'Global Active Power (kilowats', xlab='')
> lines(t$Sub_metering_2 ~ t$DateTime, col='red')
> lines(t$Sub_metering_3 ~ t$DateTime, col ='blue')
> legend('topright',col = c('black','red','blue'), lwd=c(1,1,1),c('Sub_mwetering_1','Sub_metering_2','Sub_metering_3'))
